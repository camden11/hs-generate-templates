{
  "author": "HubSpot",
  "description": "Template to open an iframe",
  "version": "1.0.0",
  "componentRoot": "app",
  "questions": [
    {
      "text": "What is the name of your app?",
      "type": "string",
      "tagValue": "appName"
    },
    {
      "text": "What is the description of your app?",
      "type": "string",
      "tagValue": "appDescription"
    },
    {
      "text": "What is the title of your card?",
      "type": "string",
      "tagValue": "cardTitle"
    },
    {
      "text": "Enter a description for the iframe.",
      "type": "string",
      "tagValue": "iframeDescription"
    },
    {
      "text": "Enter text for the button to open the iframe.",
      "type": "string",
      "tagValue": "iframeButtonText"
    },
    {
      "text": "Enter the url for the iframe",
      "type": "string",
      "tagValue": "iframeUrl"
    }
  ]
}

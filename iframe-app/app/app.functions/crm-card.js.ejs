exports.main = async (context, sendResponse) => {
  sendResponse({
    sections: [
      {
        type: 'text',
        text: '<%= iframeDescription %>',
      },
      {
        type: 'button',
        text: '<%= iframeButtonText %>',
        onClick: {
          type: 'IFRAME',
          // Width and height of the iframe (in pixels)
          width: 1000,
          height: 800,
          uri: '<%= iframeUrl %>',
        },
      },
    ],
  });
};
